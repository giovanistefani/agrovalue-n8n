    "automation",
    "aws",
    "terraform",
    "docker",
    "agriculture",
    "agrovalue",
    "serverless",
    "cost-optimization"
  ],
  "author": "AgroValue Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agrovalue/agrovalue-n8n/issues"
  },
  "homepage": "https://github.com/agrovalue/agrovalue-n8n#readme",
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "linux",
    "darwin"
  ]

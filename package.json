{
  "name": "agrovalue-n8n",
  "version": "1.0.0",
  "description": "Plataforma de automação N8N econômica e escalável para AgroValue",
  "main": "index.js",
  "scripts": {
    "start:local": "./scripts/start-local.sh",
    "stop:local": "./scripts/stop-local.sh",
    "deploy:aws": "./scripts/deploy-aws.sh",
    "backup": "./scripts/backup.sh",
    "costs": "./scripts/monitor-costs.sh",
    "logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "ps": "docker-compose -f docker/docker-compose.yml ps",
    "clean": "docker system prune -f",
    "test:local": "curl -f http://localhost:5678/healthz || exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agrovalue/agrovalue-n8n.git"
  },
  "keywords": [
